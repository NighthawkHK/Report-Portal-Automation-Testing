<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <listeners>
        <listener class-name="edu.reportportal.tests.listeners.TestNGListener"/>
    </listeners>
    <test name="Test">
        <classes>
            <class name="edu.reportportal.tests.ui.LoginTest" />
        </classes>
